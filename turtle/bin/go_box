--go_box HaNYCNZM 1478155922

--[[
  Returns to one spot above your 
  selected box and dumps inventory into 
  the box.
    > bin/go_box
    > bin/go_box 100
--]]

os.loadAPI("lib/move")
local args    = {...}
local highest = args[1] or 100
local name    = args[2] or 'box'
move.return_to_box(highest, name)
move.dump_inv("dn")

 -- vim: syntax=lua

